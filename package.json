{
  "name": "mml-react",
  "version": "0.1.1",
  "description": "MML (Message Markup Language) for React implementation.",
  "author": "tschellenbach",
  "license": "commercial",
  "repository": "tschellenbach/mml-react",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "style": "dist/css/index.css",
  "jsnext:main": "dist/index.es.js",
  "jsdelivr": "./dist/browser.full-bundle.min.js",
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "cover": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "yarn build-scripts && yarn build-styles",
    "start": "rollup -c -w",
    "build-scripts": "rollup -c",
    "watch-styles": "sass --watch src/styles/index.scss dist/css/index.css",
    "build-styles": "sass src/styles/index.scss dist/css/index.css --style compressed",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "prettier": "npx prettier \"src/**/*.js\" --write",
    "lint": "npx eslint src/**/*.js --fix",
    "docs": "npx styleguidist build",
    "docs-server": "npx styleguidist server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@braintree/sanitize-url": "^4.0.0",
    "@rgrove/parse-xml": "^2.0.1",
    "date-fns": "^2.16.1",
    "ical-expander": "^2.1.0",
    "linkifyjs": "^2.1.9",
    "prop-types": "^15.7.2",
    "pure-react-carousel": "^1.27.5",
    "react-add-to-calendar": "^0.1.5",
    "react-markdown": "^4.2.2",
    "react-virtuoso": "^0.20.0"
  },
  "peerDependencies": {
    "react": "^16.8.x",
    "react-dom": "^16.8.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.5",
    "@babel/preset-react": "^7.7.4",
    "@stream-io/rollup-plugin-node-builtins": "^2.1.1",
    "@svgr/rollup": "^4.3.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.1.1",
    "husky": "^3.1.0",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.3.0",
    "react-styleguidist": "^10.3.2",
    "react-test-renderer": "^16.12.0",
    "rollup": "^1.27.8",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy-glob": "^0.3.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-url": "^3.0.1",
    "sass": "^1.27.0",
    "stream-chat-react": "^3.1.6"
  },
  "files": [
    "dist"
  ],
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ]
}
